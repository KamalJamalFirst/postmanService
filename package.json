{
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "migration:generateInitial": "npm run typeorm migration:generate ./src/migrations/InitialMigration -d ./src/data-source.ts",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "seed": "ts-node ./src/seeds/seed.ts",
    "prestart": "npm run migration:generateInitial && npm run seed"
  },
  "dependencies": {
    "libphonenumber-js": "^1.11.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "@ngrok/ngrok": "^1.4.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.2.0",
    "ajv": "^8.17.1",
    "body-parser": "^1.20.3",
    "express": "^4.19.2",
    "mysql2": "^3.11.3",
    "npm": "^10.9.0",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.5.4"
  }
}
